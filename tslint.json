{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "object-curly-spacing": "always"
  },
  "rulesDirectory": []
}
